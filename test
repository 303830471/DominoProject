		            		  if(comCode!=undefined) document.getElementById(comCode).value = result["BUKRS"];
		            		  if(city!=undefined) document.getElementById(city).value = result["ORT01"];
		            	  }
			          }
			        };
		 digiflow.createGridDialog(param);
		 return true;
	},
